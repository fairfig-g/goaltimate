---
---
<section>
    <div>
        <hgroup>
            <h2>Contact us</h2>
            <h3>Do you have any questions or comments? Please get in touch!</h3>
        </hgroup>
        
    </div>
</section>
<section>
    <form>
        <fieldset>
          <label>
            Your name
            <input
              name="name"
              placeholder="Stay Awesome"
              autocomplete="name"
            />
          </label>
          <label>
            Email
            <input
              type="email"
              name="email"
              placeholder="stayawesome@goaltimate.org"
              autocomplete="email"
            />
          </label>
          <label>
            Your message
            <textarea
            name="message"
            placeholder="Your message here..."
            aria-label="Message"
            >
            </textarea>
          </label>  
        </fieldset>
      
        <input
          type="submit"
          value="Send"
        />
      </form>
</section>
